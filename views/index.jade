extends layout

block content
  h1.text-xs-center.text-primary.py-3= title
  p.lead Only awesome people here...
  if user
    p= user
    img(src="#{user.profileImage}" width="200px" height=" 200px")
  
  div.container
    each video, i in videos
      div.col-xs-4 #{video}
        video(controls width="300px" height="250px")
          source(src="#{video.url}")
      
